{
  "name": "g2-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:export": "node scripts/build-export.mjs",
    "deploy": "pnpm run build:export && pnpm run deploy:s3 && pnpm run deploy:cloudfront",
    "deploy:cloudfront": "aws cloudfront create-invalidation --distribution-id E1C1UNHJ75JZMZ --paths \"/*\"",
    "deploy:s3": "aws s3 sync ./out s3://g2-frontend-ver2 --delete --exclude '*.txt'",
    "deploy:clone": "node scripts/deploy-clone.mjs",
    "quick-deploy": "node scripts/quick-deploy.mjs",
    "full-deploy": "node scripts/full-deploy.mjs",
    "backend-deploy": "cd backend && zip -r enhanced-chatbot.zip lambda/ && aws lambda update-function-code --function-name sedaily-chatbot-dev-handler --zip-file fileb://enhanced-chatbot.zip",
    "setup:aws-clone": "node scripts/setup-aws-clone.mjs",
    "setup:custom-domain": "node scripts/setup-custom-domain.mjs",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start"
  },
